{
  "extends": "airbnb",
  "rules": {
    "semi": "off",
    "no-plusplus": "off",
    "no-console": "warn",
    "yoda": [ "error", "never", { "onlyEquality": true } ],
    "no-magic-numbers": "error",
    "no-unused-vars": "error",
    "comma-dangle": "off",
    "eol-last": "error",
    "indent": [ "error", 2 ],
    "multiline-comment-style": ["warn", "starred-block" ],
    "no-inline-comments": "error"
  }
}
